#include<iostream>
using namespace std;

int main()
{
    int i,n;   //定义两个整形变量
    cout<<"请输入一个大于1的整数"<<endl;
    cin>>n;                             //获取n的值
    for(i=2;i<(n/2);i++){
      if(n%i=0){                                  
        cout<<"你输入的数"<<n<<"不是素数"<<endl;
        return 0;
        }
    }
    cout<<"你输入的数"<<n<<"是素数"<<endl;
    return 0;
  }
